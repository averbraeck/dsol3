<?xml version="1.0"?>

<document>
	<properties>
		<author email="a.verbraeck@tudelft.nl">Alexander Verbraeck</author>
		<title>changes to JSTATS</title>
	</properties>
	<body>
		<release version="2.0.13" date="2009-06-06">
			<action dev="alexandv" type="add">Split jstats in a math part and a charting part</action>
		</release>
		<release version="2.0.12" date="2009-05-23">
			<action dev="alexandv" type="add">Updates for java 1.5</action>
		</release>
		<release version="2.0.11" date="2007-11-10">
			<action dev="alexandv" type="update">Changed directory structure for Maven 2</action>
			<action dev="alexandv" type="fix">Serializability added for a few classes</action>
			<action dev="alexandv" type="update">serialVersionUID field added for most classes</action>
		</release>
		 <release version="2.0.0-ALPHA" date="2005-06-29">
			<action dev="PeterJacobs" type="update">Updated to JDK 5.0</action>
		</release>
		<release version="1.6.5" date="2004-12-15">
			<action dev="PeterJacobs" type="update">License has changed from GPL to LGPL</action>
			<action dev="PeterJacobs" type="update">Code has been reformatted and @uml tags are removed</action>			
			<action dev="PeterJacobs" type="add">Serializability has been added in a number of classes</action>						
        </release>
        <release version="1.6.4" date="2004-10-25">
			<action dev="PeterJacobs" type="add">Filters are added to jstats</action>
		</release>
		<release version="1.6.3" date="2004-10-16">
			<action dev="PeterJacobs" type="fix">Counter fixed</action>
		</release>
		<release version="1.6.2" date="2004-09-29">
			<action dev="PeterJacobs" type="fix">@uml property in javadoc 
				removed</action>
			<action dev="PeterJacobs" type="fix">Counter#getTable() has now a 
				weak reference</action>
			<action dev="PeterJacobs" type="fix">Tally#getTable() has now a 
				weak reference</action>
			<action dev="PeterJacobs" type="fix">Project dependencies</action>
		</release>
		<release version="1.6.1" date="2004-09-28">
			<action dev="PeterJacobs" type="add">JSTATS became a maven 
				project</action>
		</release>
	</body>
</document>